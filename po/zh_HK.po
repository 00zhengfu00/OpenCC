#: src/tools/opencc.c:92 src/tools/opencc_dict.c:342
#, c-format
msgid "Usage:\n"
msgstr "使用方法：\n"

#: src/tools/opencc.c:93
#, c-format
msgid ""
"  opencc [-i input_file] [-o output_file] [-c config_file]\n"
"\n"
msgstr ""
"  opencc [-i input_file] [-o output_file] [-c config_file]\n"
"\n"

#: src/tools/opencc.c:94 src/tools/opencc_dict.c:344
#, c-format
msgid "    -i input_file\n"
msgstr "    -i input_file\n"

#: src/tools/opencc.c:95
#, c-format
msgid "      Read original text from input_file.\n"
msgstr "      從 input_file 讀取原始文本。\n"

#: src/tools/opencc.c:96 src/tools/opencc_dict.c:346
#, c-format
msgid "    -o output_file\n"
msgstr "    -o output_file\n"

#: src/tools/opencc.c:97
#, c-format
msgid "      Write converted text to output_file.\n"
msgstr "      將轉換後的文本寫入output_file。\n"

#: src/tools/opencc.c:98
#, c-format
msgid "    -c config_file\n"
msgstr "    -c config_file\n"

#: src/tools/opencc.c:99
#, c-format
msgid "      Load dictionary configuration from config_file.\n"
msgstr "      從config_file讀取配置。\n"

#: src/tools/opencc.c:100 src/tools/opencc.c:105 src/tools/opencc.c:106
#: src/tools/opencc_dict.c:239 src/tools/opencc_dict.c:247
#: src/tools/opencc_dict.c:348 src/tools/opencc_dict.c:349
#, c-format
msgid "\n"
msgstr "\n"

#: src/tools/opencc.c:101
#, c-format
msgid "  Note:\n"
msgstr "  註釋：\n"

#: src/tools/opencc.c:102
#, c-format
msgid ""
"    Text from standard input will be read if input_file is not set\n"
"    and will be written to standard output if output_file is not set.\n"
msgstr ""
"    如果沒有設置input_file，將會從標準輸入讀取文本。如果\n"
"    沒有設置output_file，將會把轉換後文本寫入到標準輸出。\n"

#: src/tools/opencc.c:104
#, c-format
msgid "    Default configuration(%s) will be load if config_file is not set.\n"
msgstr "    如果沒有設置config_file，則會讀取默認配置文件(%s)。\n"

#: src/tools/opencc_dict.c:336
#, c-format
msgid ""
"\n"
"Open Chinese Convert (OpenCC) Dictionary Tool\n"
"Version %s\n"
"\n"
msgstr ""
"\n"
"Open Chinese Convert (OpenCC) 辭典工具\n"
"版本 %s\n"
"\n"

#: src/tools/opencc_dict.c:343
#, c-format
msgid ""
"  opencc_dict -i input_file -o output_file\n"
"\n"
msgstr ""
"  opencc_dict -i input_file -o output_file\n"
"\n"

#: src/tools/opencc_dict.c:345
#, c-format
msgid "      Read data from input_file.\n"
msgstr "      從input_file讀取數據。\n"

#: src/tools/opencc_dict.c:347
#, c-format
msgid "      Write converted data to output_file.\n"
msgstr "      將生成的辭典寫入output_file。\n"

#: src/tools/opencc_dict.c:395
#, c-format
msgid "Please specify input file using -i.\n"
msgstr "請使用-i指定輸入文件。\n"

#: src/tools/opencc_dict.c:402
#, c-format
msgid "Please specify output file using -o.\n"
msgstr "請使用-o指定輸入文件。\n"
