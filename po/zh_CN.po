#: src/tools/opencc.c:94 src/tools/opencc_dict.c:333
#, c-format
msgid "Usage:\n"
msgstr "使用方法：\n"

#: src/tools/opencc.c:95
#, c-format
msgid ""
"  opencc [-i input_file] [-o output_file] [-c config_file]\n"
"\n"
msgstr ""
"  opencc [-i input_file] [-o output_file] [-c config_file]\n"
"\n"

#: src/tools/opencc.c:96 src/tools/opencc_dict.c:335
#, c-format
msgid "    -i input_file\n"
msgstr "    -i input_file\n"

#: src/tools/opencc.c:97
#, c-format
msgid "      Read original text from input_file.\n"
msgstr "      从 input_file 读取原始文本。\n"

#: src/tools/opencc.c:98 src/tools/opencc_dict.c:337
#, c-format
msgid "    -o output_file\n"
msgstr "    -o output_file\n"

#: src/tools/opencc.c:99
#, c-format
msgid "      Write converted text to output_file.\n"
msgstr "      将转换后的文本写入output_file。\n"

#: src/tools/opencc.c:100
#, c-format
msgid "    -c config_file\n"
msgstr "    -c config_file\n"

#: src/tools/opencc.c:101
#, c-format
msgid "      Load dictionary configuration from config_file.\n"
msgstr "      从config_file读取配置。\n"

#: src/tools/opencc.c:102 src/tools/opencc.c:107 src/tools/opencc.c:108
#: src/tools/opencc_dict.c:243 src/tools/opencc_dict.c:339
#: src/tools/opencc_dict.c:340
#, c-format
msgid "\n"
msgstr "\n"

#: src/tools/opencc.c:103
#, c-format
msgid "  Note:\n"
msgstr "  注释：\n"

#: src/tools/opencc.c:104
#, c-format
msgid ""
"    Text from standard input will be read if input_file is not set\n"
"    and will be written to standard output if output_file is not set.\n"
msgstr ""
"    如果没有设置input_file，将会从标准输入读取文本。如果\n"
"    没有设置output_file，将会把转换后文本写入到标准输出。\n"

#: src/tools/opencc.c:106
#, c-format
msgid "    Default configuration(%s) will be load if config_file is not set.\n"
msgstr "    如果没有设置config_file，则会读取默认配置文件(%s)。\n"

#: src/tools/opencc_dict.c:327
#, c-format
